astr = "Wow am very happy",
bstr = "_____________Happies____________",

ainst = astr.charCodeAt(
	t >> 5 & t << 2 * (t >> 6 + t >> 8) * t >> 12
)*(astr.length)*t,

binst = astr.charCodeAt(
	t >> 4 ^ t << 2 + (t >> 5 * t + t >> 6) * t >> 12
)*(astr.length)*t,

ainst ^ binst
