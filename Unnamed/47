astr = "Wow am very Happy",
bstr = "IPTV service provider",
cstr = "$$$$$*!!$$",

ainst = astr.charCodeAt(
  (
    ((t & (t >> 6)) & 12) + (t >> 2) * ((t >> 14 & 6) ^ (t >> 18 & 2))
  ) % (astr.length)
),

binst = bstr.charCodeAt(
  (
    ((t & (t >> 8)) & 4 ^ 20) + t + (t / 6) * ((t >> 8 & 16) ^ (t >> 7 & 17))
  ) % (bstr.length)
),

cinst = cstr.charCodeAt(
  (
    (t & (t >> 14)) + ((t * 1.058) >> t) * ((t >> 9 & 12) ^ (t >> 12 & 18))
  ) % (cstr.length)
),

(ainst + binst + cinst) / 2 | (t >> 17 ? t >> 4 : t >> 10 >= 124 ? 120 : 0)
https://dollchan.net/bytebeat/#
