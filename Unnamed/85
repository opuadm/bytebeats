a = ((t >> 4 ^ t >> 6) - ((((t % 12) < 4) ? (t >> 8 & t << 2) : -(t >> 8 & t << 2)) - ((t >> 4) | (t >> 8 & t << 2)))),
b = (a >> (1 - (t >> 10 & t << 1))),
b + (b >> 2)
